{
  "scopeName": "bigquery.injection",
  "injectionSelector": "L:source.bigquery -string -comment",
  "patterns": [
    {
      "include": "#number-sign-comment"
    }
  ],
  "repository": {
    "number-sign-comment": {
      "match": "#[^\n]*",
      "name": "comment.line.number-sign.bigquery"
    }
  }
}
